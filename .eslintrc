{
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "ecmaFeatures": {
      "blockBindings": true,
      "forOf": true,
      "arrowFunctions": true,
      "classes": true,
      "modules": true,
      "defaultParams": true,
      "destructuring": true,
      "templateString": true,
      "superInFunctions": true,
      "spread": true
    },
    "parserOptions": {
      "sourceType": "module"
    },
    "rules": {
        "no-use-before-define": [2],
        "strict": [0, "global"]
    },
    "globals": {
      "angular": false
    }
}

